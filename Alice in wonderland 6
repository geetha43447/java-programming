import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int x=n, c=0;
        while(n>0) {
            int a=n%10;
            c+=a;
            n/=10;
        }
        System.out.println("Bird said:"+x+"\nAlice must go in path-"+c);
    }
}
