import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt(),res=0;
        if(n<9) {
            System.out.println("STDOUT");
            return;
        }
        else if(n>=100) {
            while(n>=100) n/=10;
        }
        while(n>0) {
            int r=n%10;
            res=res*10+r;
            n/=10;
        }
        System.out.println(res);
         
    }
}
