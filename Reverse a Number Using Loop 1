import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        if(n<0) System.out.println("Invalid Input");
        else {
            int x=0;
            while(n>0) {
                int rem=n%10;
                x=x*10+rem;
                n/=10;
            }
            System.out.println(x);
        }
    }
}
