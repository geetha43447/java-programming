import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int x1=sc.nextInt();
        int y1=sc.nextInt();
        int x2=sc.nextInt();
        int y2=sc.nextInt();
        System.out.print("Binoy's house is located at ");
        System.out.printf("(%.1f,%.1f)",((float)(x1+x2)/2),((float)(y1+y2)/2));
    }
}
