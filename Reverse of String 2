import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) throws IOException {
        BufferedReader bf=new BufferedReader(new InputStreamReader(System.in));
        String str=bf.readLine(),rstr="";
        int n=str.length();
        for(int i=n-1;i>=0;i--) {
            rstr+=str.charAt(i);
        }
        System.out.println(rstr);
    }
}
