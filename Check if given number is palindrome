import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        if(n>=100 && n<=999) {
            int a=n/100,b=n%10;
            if(a==b) System.out.println("palindrome");
            else System.out.println("not palindrome");
        }
        else System.out.println("Invalid Input");
    }
}
