import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String str=sc.nextLine();
        for(int i=0;i<str.length();i++) {
            char a=str.charAt(i);
            if(a=='a' || a=='e' || a=='i' || a=='o' || a=='u') continue;
            else System.out.print(a);
        }
    }
}
